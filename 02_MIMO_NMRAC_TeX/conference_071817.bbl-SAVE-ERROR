    \entry{mareelsRevisitingMitRule1987}{article}{}{}
      \name{author}{5}{}{%
        {{hash=c614fc54da2684b26524ac6632b282a2}{%
           family={Mareels},
           familyi={M\bibinitperiod},
           given={Iven\bibnamedelimb M.\bibnamedelimi Y.},
           giveni={I\bibinitperiod\bibinitdelim M\bibinitperiod\bibinitdelim Y\bibinitperiod}}}%
