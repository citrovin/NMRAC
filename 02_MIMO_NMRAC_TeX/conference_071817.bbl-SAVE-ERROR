    \entry{congPIDLikeNeuralNetwork2009}{article}{}{}
      \name{author}{2}{}{%
        {{hash=deaed8dd936d7f8758884dcad6b13797}{%
           family={Cong},
           familyi={C\bibinitperiod},
           given={S.},
